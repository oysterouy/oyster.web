@using demotheme;
@using oyster.web;
@{dynamic Model=null;}
<div>
    <span>姓名：@Model.Name</span> <span>年龄：@(Settings.aa + 1203)</span>
    @{
        for (int i = 0; i < 6; i++)
        {
        <p>@i</p>
        <div>
            aaaa</div>
        @{i += 2;}
         }
        <p>
            HHHHHHHHHHHHh</p> }
</div>
@oyster.web.TemplateHelper.Init((request) =>
{
    return new object[] { "张三"};
})
@oyster.web.TemplateHelper.Request((string name, Response resp) =>
{
    resp.Model.Name=name;
})
